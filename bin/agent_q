#!/usr/bin/env ruby
require 'rubygems'
require_relative '../lib/check_case'

subcommand = ARGV[0]

if subcommand == 'checkcase'
  quepid_case = ARGV[1]
  threshold_score = ARGV[2]
  username = ARGV[3]
  password = ARGV[4]
  quepid_url = ARGV[5]
  
  
  
  CheckCase.new(quepid_case, threshold_score, username, password, quepid_url).run
end
